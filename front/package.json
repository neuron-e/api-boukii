{
  "name": "boukii-admin-v5",
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "ng": "ng",
    "start": "npm run config:development && ng serve",
    "start:local": "npm run config:local && ng serve --configuration=local",
    "start:develop": "npm run config:develop && ng serve --configuration=develop",
    "start:staging": "npm run config:staging && ng serve --configuration=staging",
    "start:production": "npm run config:production && ng serve --configuration=production",
    "start:test": "npm run config:test && ng serve --configuration=test",
    "build": "npm run config:development && ng build",
    "build:local": "npm run config:local && ng build --configuration=local",
    "build:development": "npm run config:development && ng build --configuration=development",
    "build:develop": "npm run config:develop && ng build --configuration=develop",
    "build:staging": "npm run config:staging && ng build --configuration=staging",
    "build:production": "npm run config:production && ng build --configuration=production",
    "build:test": "npm run config:test && ng build --configuration=test",
    "watch": "ng build --watch --configuration development",
    "config:local": "node scripts/configure-environment.js local",
    "config:development": "node scripts/configure-environment.js development",
    "config:develop": "node scripts/configure-environment.js develop",
    "config:staging": "node scripts/configure-environment.js staging",
    "config:production": "node scripts/configure-environment.js production",
    "config:test": "node scripts/configure-environment.js test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --runInBand",
    "test:karma": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "format:staged": "lint-staged",
    "format:verify": "node scripts/format-check.js",
    "verify": "npm run lint && npm run typecheck && npm run test:ci && npm run build",
    "code-quality": "npm run lint && npm run format:verify && npm run typecheck",
    "code-quality:fix": "npm run lint:fix && npm run format:write && npm run typecheck",
    "commit": "npx git-cz",
    "commit:retry": "npx git-cz --retry",
    "hooks:install": "node scripts/git-hooks-manager.js install",
    "hooks:validate": "node scripts/git-hooks-manager.js validate",
    "hooks:test": "node scripts/git-hooks-manager.js test",
    "hooks:status": "node scripts/git-hooks-manager.js status",
    "analyze": "npm run analyze:bundle && npm run analyze:deps && npm run analyze:code",
    "analyze:bundle": "ng build --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "analyze:deps": "npx depcheck && npx npm-check-updates",
    "analyze:code": "node scripts/quality-analyzer.js",
    "metrics": "npm run analyze:code",
    "quality:report": "npm run code-quality && npm run analyze:code",
    "sonar": "npx sonar-scanner",
    "sonar:analyze": "node scripts/sonar-quality-analyzer.js analyze",
    "sonar:prepare": "node scripts/sonar-quality-analyzer.js prepare",
    "sonar:validate": "node scripts/sonar-quality-analyzer.js validate",
    "sonar:report": "node scripts/sonar-quality-analyzer.js report",
    "sonar:metrics": "node scripts/sonar-quality-analyzer.js metrics",
    "typecheck": "tsc -p tsconfig.app.json --noEmit",
    "preview": "ng serve --configuration production",
    "storybook": "ng run boukii-admin-v5:storybook",
    "build:storybook": "storybook build",
    "prepare": "husky",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "cypress:run:headed": "cypress run --headed",
    "cypress:run:ci": "cypress run --browser chrome --headless --reporter junit --reporter-options mochaFile=cypress/reports/test-results.xml",
    "e2e:auth": "cypress run --spec 'cypress/e2e/auth-flow.cy.ts'",
    "e2e:errors": "cypress run --spec 'cypress/e2e/error-handling.cy.ts'",
    "e2e:theme": "cypress run --spec 'cypress/e2e/theme-switching.cy.ts'",
    "e2e:smoke": "cypress run --spec 'cypress/e2e/auth-flow.cy.ts' --env grepTags='@smoke'",
    "test:e2e": "start-server-and-test start http://localhost:4200 cypress:run",
    "test:e2e:ci": "start-server-and-test start http://localhost:4200 cypress:run:ci"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.html": [
      "prettier --write",
      "git add"
    ],
    "*.{scss,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ],
    "*.spec.ts": [
      "eslint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests --passWithNoTests",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^18.0.0",
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/schematic": "^4.1.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/angular": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.0.0",
    "angular-eslint": "20.1.1",
    "cypress": "^14.5.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^0.8.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.13",
    "storybook": "^8.6.14",
    "ts-jest": "^29.4.1",
    "typescript": "~5.5.0",
    "typescript-eslint": "8.34.1"
  }
}
